{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx,js,mjs}",
    "tests/**/*.{ts,tsx}"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx,js,mjs}",
    "tests/**/*.{ts,tsx}",
    "prisma/**/*.ts"
  ],
  "paths": {
    "@": [
      "."
    ],
    "@/components": [
      "components"
    ],
    "@/app": [
      "app"
    ],
    "@/lib": [
      "lib"
    ]
  },
  "ignoreDependencies": [
    "@radix-ui/react-dialog",
    "@radix-ui/react-slot",
    "@radix-ui/react-tabs",
    "clsx",
    "react-markdown",
    "remark-gfm",
    "zod",
    "tailwind-merge"
  ],
  "ignore": [
    "archive/legacy-ui/**",
    "prisma/seed.ts",
    "scripts/route-audit.mjs"
  ]
}
