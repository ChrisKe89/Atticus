    "/admin/dictionary": {
      "get": {
        "operationId": "read_dictionary_admin_dictionary_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DictionaryPayload"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Read Dictionary"
      },
